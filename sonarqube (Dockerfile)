#sonarqube image pulling from dockerhub
docker pull sonarqube
#To check docker images
docker images
#Creating bridge network
docker network create LAN --driver bridge
#To check network
docker inspect LAN
#To run docker container
docker run -td --name sonarqube --network LAN -p 8082:9000 sonarqube


